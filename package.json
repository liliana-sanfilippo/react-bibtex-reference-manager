{
  "name": "@liliana-sanfilippo/react-bibtex-reference-manager",
  "version": "0.2.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "React component that functions as a reference generator and manager and render bibtex sources for scientific blogging and iGEM wikis.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "tsc -p tsconfig.test.json && mocha \"dist/test/**/*.js\"",
    "patch": "npm version patch && rm -rf dist && npm run build && npm pack && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liliana-sanfilippo/react-bibtex-reference-manager"
  },
  "keywords": [
    "react",
    "bibtex",
    "component",
    "citations",
    "reference manager"
  ],
  "author": "Liliana Sanfilippo",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@liliana-sanfilippo/bibtex-ts-parser": "^2.0.8",
    "@liliana-sanfilippo/react-bibtex-reference-generator": "^0.1.7"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^19.2.0",
    "chai": "^4.3.6",
    "jsdom": "^27.0.0",
    "mocha": "^10.8.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
